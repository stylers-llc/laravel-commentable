{
    "name": "stylers/laravel-commentable",
    "description": "Commentable Polymorphic Eloquent Models for for Laravel",
    "keywords": [
        "laravel",
        "framework",
        "Laravel-Commentable",
        "Laravel Commentable"
    ],
    "license": "MPL-2.0",
    "authors": [
        {
            "name": "Brian Faust",
            "email": "hello@basecode.sh",
            "homepage": "https://basecode.sh"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^8.0",
        "illuminate/database": "^8.0",
        "kalnoy/nestedset": "^5.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "Stylers\\Commentable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stylers\\Tests\\Commentable\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Stylers\\Commentable\\CommentableServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
